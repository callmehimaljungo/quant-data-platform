# Cloudflare R2 Configuration
# Copy this file to .env and fill in your actual credentials

# R2 Endpoint URL (e.g., https://account-id.r2.cloudflarestorage.com)
R2_ENDPOINT=your_r2_endpoint_here

# R2 Access Key ID
R2_ACCESS_KEY=your_access_key_here

# R2 Secret Access Key
R2_SECRET_KEY=your_secret_key_here

# R2 Bucket Name
R2_BUCKET=your_bucket_name_here

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Optional: Database Connection (if using external DB)
# DB_HOST=localhost
# DB_PORT=5432
# DB_NAME=quant_platform
# DB_USER=postgres
# DB_PASSWORD=your_password_here
